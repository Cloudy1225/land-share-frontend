<!--pages/setting/index.wxml-->
<view class="setting">

<view class="page__setting">

    <wux-wing-blank size="large">

    <wux-white-space size="large" />
    <wux-cell-group >
        <wux-cell title="我的实名信息"
                    is-link hasLine
            url="/pages/realName/index"
        ></wux-cell>
        <wux-cell title="电话" extra="18888888888"
        hasLine
        ></wux-cell>
        <wux-cell title="我的地址"
        is-link
        hasLine
        ></wux-cell>
    </wux-cell-group>

    <wux-white-space size="large" />
    <wux-cell-group >
        <wux-cell title="版本号" extra="1.0.0"
        hasLine
            ></wux-cell>
        <wux-cell title="隐私政策"
            is-link
            hasLine
        ></wux-cell>
        <wux-cell title="注销账号"
            is-link
            hasLine
            bindtap="cancelAccount"
        ></wux-cell>
    </wux-cell-group>

    <wux-white-space size="large" />
    <wux-button block type="assertive"
        wx:if="{{app.globaldata.role!=0}}"
        bindtap="logout"
        >退出登录

    </wux-button>


</wux-wing-blank>

</view>
</view>
